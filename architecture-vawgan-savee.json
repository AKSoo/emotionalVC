{
    "mode": "VAWGAN",    
    "hwc": [513, 1, 1],
    "z_dim": 128,
    "y_dim": 8,
    "y_emb_dim": 128,
    "discriminator": {
        "kernel": [7, 7, 115],
        "stride": [3, 3, 3],
        "output": [16, 32, 64],
        "l2-reg": 1e-6
    },
    "encoder": {
        "kernel": [7, 7, 7, 7, 7],
        "stride": [3, 3, 3, 3, 3],
        "output": [16, 32, 64, 128, 256],
        "l2-reg": 1e-6
    },
    "generator": {
        "merge_dim": 171,
        "init_out": 64,
        "kernel": [9, 7, 7, 1025],
        "stride": [3, 3, 3, 1],
        "output": [32, 16, 8, 1],
        "l2-reg": 1e-6
    },
    "training": {
        "src_dir": "./dataset/savee/bin/*/SNeu/*.bin",
        "trg_dir": "./dataset/savee/bin/*/THap/*.bin",
        "batch_size": 128,
        "lr": 1e-4,
        "nIterD": 5,
        "lambda": 10,
        "max_iter": 200000,
        "epoch_vae": 0,
        "epoch_vawgan": 30,
        "gamma": 50,
        "n_unroll": 5,
        "n_unroll_intense": 100
    }
}
